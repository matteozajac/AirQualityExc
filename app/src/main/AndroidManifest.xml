<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.matteozajac.airqualityexcercise">

    <application
        android:allowBackup="true"
        android:name="com.matteozajac.airqualityexcercise.AirQualityApp"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AirQualityExcercise"
        tools:targetApi="31">
        <activity
            android:name="com.matteozajac.airqualityexcercise.ui.aqList.AqListActivity"
            android:exported="true"
            android:theme="@style/Theme.AirQualityExcercise">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.matteozajac.airqualityexcercise.ui.mvpSample.MVPActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat">
        </activity>
    </application>

</manifest>